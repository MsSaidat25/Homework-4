      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 1000
        },
        "id": "a5231d6e",
        "outputId": "162eccae-2541-404e-901c-725204d37266"
      },
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from scipy.cluster.hierarchy import dendrogram, linkage\n",
        "from scipy.spatial.distance import squareform\n",
        "import pandas as pd\n",
        "\n",
        "print(\"=\"*70)\n",
        "print(\"QUESTION 4: HIERARCHICAL CLUSTERING AND DBSCAN\")\n",
        "print(\"=\"*70)\n",
        "\n",
        "# Distance matrix from the homework\n",
        "distance_matrix = np.array([\n",
        "    [0, 5, 8, 4, 7, 7, 8, 2],  # a\n",
        "    [5, 0, 6, 4, 5, 4, 3, 4],  # b\n",
        "    [8, 6, 0, 5, 1, 2, 7, 6],  # c\n",
        "    [4, 4, 5, 0, 4, 4, 7, 1],  # d\n",
        "    [7, 5, 1, 4, 0, 1, 7, 5],  # e\n",
        "    [7, 4, 2, 4, 1, 0, 5, 5],  # f\n",
        "    [8, 3, 7, 7, 7, 5, 0, 8],  # g\n",
        "    [2, 4, 6, 1, 5, 5, 8, 0]   # h\n",
        "])\n",
        "\n",
        "points = ['a', 'b', 'c', 'd', 'e', 'f', 'g', 'h']\n",
        "\n",
        "# Display the distance matrix in table format\n",
        "print(\"\\nDistance Matrix (Table Format):\")\n",
        "print(\"-\" * 70)\n",
        "df_distance = pd.DataFrame(distance_matrix, index=points, columns=points)\n",
        "print(df_distance)\n",
        "print(\"\\nNote: This is a symmetric matrix where distance(i,j) = distance(j,i)\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*70)\n",
        "print(\"PART (a): HIERARCHICAL CLUSTERING WITH SINGLE LINKAGE\")\n",
        "print(\"=\"*70)\n",
        "\n",
        "# Convert to condensed distance matrix for scipy\n",
        "condensed_dist = squareform(distance_matrix)\n",
        "\n",
        "# Perform hierarchical clustering with single linkage\n",
        "linkage_matrix = linkage(condensed_dist, method='single')\n",
        "\n",
        "# Print the linkage matrix with explanation\n",
        "print(\"\\nLinkage Matrix:\")\n",
        "print(\"-\" * 70)\n",
        "print(\"Each row shows: [cluster1_id, cluster2_id, distance, num_items_in_new_cluster]\")\n",
        "print(\"(Points 0-7 represent a-h, clusters 8+ are merged clusters)\\n\")\n",
        "print(pd.DataFrame(linkage_matrix,\n",
        "                   columns=['Cluster 1', 'Cluster 2', 'Distance', 'Size'],\n",
        "                   index=[f'Step {i+1}' for i in range(len(linkage_matrix))]))\n",
        "\n",
        "# Show merge order step by step\n",
        "print(\"\\n\\nStep-by-Step Merge Process:\")\n",
        "print(\"-\" * 70)\n",
        "\n",
        "def get_cluster_name(idx, points):\n",
        "    \"\"\"Helper function to get readable cluster names\"\"\"\n",
        "    if idx < len(points):\n",
        "        return points[int(idx)]\n",
        "    else:\n",
        "        return f\"Cluster{int(idx)}\"\n",
        "\n",
        "for i, (c1, c2, dist, size) in enumerate(linkage_matrix):\n",
        "    # Map cluster indices to point names\n",
        "    if c1 < 8:\n",
        "        name1 = points[int(c1)]\n",
        "    else:\n",
        "        name1 = f\"merged_cluster_{int(c1)}\"\n",
        "\n",
        "    if c2 < 8:\n",
        "        name2 = points[int(c2)]\n",
        "    else:\n",
        "        name2 = f\"merged_cluster_{int(c2)}\"\n",
        "\n",
        "    print(f\"Step {i+1}: Merge {name1} and {name2} at distance {dist:.1f} → New cluster size: {int(size)}\")\n",
        "\n",
        "# Create the dendrogram\n",
        "print(\"\\n\\nGenerating Dendrogram...\")\n",
        "plt.figure(figsize=(12, 7))\n",
        "dendrogram(linkage_matrix,\n",
        "           labels=points,\n",
        "           leaf_font_size=14,\n",
        "           color_threshold=0)\n",
        "plt.title('Hierarchical Clustering Dendrogram (Single Linkage)',\n",
        "          fontsize=16, fontweight='bold', pad=20)\n",
        "plt.xlabel('Data Points', fontsize=14, fontweight='bold')\n",
        "plt.ylabel('Distance', fontsize=14, fontweight='bold')\n",
        "plt.grid(axis='y', alpha=0.3, linestyle='--')\n",
        "plt.tight_layout()\n",
        "\n",
        "# Save the dendrogram\n",
        "import os\n",
        "save_path = '/content/dendrogram_single_linkage.png'\n",
        "plt.savefig(save_path, dpi=300, bbox_inches='tight')\n",
        "print(f\"✓ Dendrogram saved as '{save_path}'\")\n",
        "print(f\"✓ File exists: {os.path.exists(save_path)}\")\n",
        "plt.show()\n",
        "\n",
        "print(\"\\n\" + \"=\"*70)\n",
        "print(\"PART (b): DBSCAN CORE POINT IDENTIFICATION\")\n",
        "print(\"=\"*70)\n",
        "print(f\"\\nParameters: ε (epsilon) = 6, minPts = 2\")\n",
        "print(\"Definition: A point is CORE if at least minPts points are within distance ε\")\n",
        "print(\"-\" * 70)\n",
        "\n",
        "epsilon = 6\n",
        "min_pts = 2\n",
        "\n",
        "core_points = []\n",
        "core_status = {}\n",
        "\n",
        "for i, point in enumerate(points):\n",
        "    # Find all neighbors within epsilon distance (including the point itself)\n",
        "    neighbors = []\n",
        "    neighbor_distances = []\n",
        "\n",
        "    for j, other_point in enumerate(points):\n",
        "        if distance_matrix[i][j] <= epsilon:\n",
        "            neighbors.append(other_point)\n",
        "            neighbor_distances.append(distance_matrix[i][j])\n",
        "\n",
        "    # Check if it's a core point\n",
        "    is_core = len(neighbors) >= min_pts\n",
        "    core_status[point] = is_core\n",
        "\n",
        "    if is_core:\n",
        "        core_points.append(point)\n",
        "\n",
        "    # Print detailed information for each point\n",
        "    print(f\"\\nPoint {point}:\")\n",
        "    print(f\"  Neighbors within ε={epsilon}: {neighbors}\")\n",
        "    print(f\"  Distances: {neighbor_distances}\")\n",
        "    print(f\"  Neighbor count: {len(neighbors)}\")\n",
        "    print(f\"  Status: {'✓ CORE POINT' if is_core else '✗ NOT CORE'}\")\n",
        "\n",
        "# Summary\n",
        "print(\"\\n\" + \"=\"*70)\n",
        "print(\"SUMMARY OF RESULTS\")\n",
        "print(\"=\"*70)\n",
        "\n",
        "print(\"\\nPart (a) - Hierarchical Clustering:\")\n",
        "print(f\"  • Method: Single Linkage (minimum distance)\")\n",
        "print(f\"  • Final dendrogram saved to: {save_path}\")\n",
        "print(f\"  • First merges at distance 1: (e,f), (c,e,f), (d,h)\")\n",
        "print(f\"  • Final merge at distance 4\")\n",
        "\n",
        "print(\"\\nPart (b) - DBSCAN Core Points:\")\n",
        "print(f\"  • Parameters: ε = {epsilon}, minPts = {min_pts}\")\n",
        "print(f\"  • Core points: {core_points}\")\n",
        "print(f\"  • Total: {len(core_points)}/{len(points)} points are CORE\")\n",
        "\n",
        "if len(core_points) == len(points):\n",
        "    print(f\"  • Result: ALL points are core points!\")\n",
        "else:\n",
        "    non_core = [p for p in points if p not in core_points]\n",
        "    print(f\"  • Non-core points: {non_core}\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*70)\n",
        "print(\"✓ ANALYSIS COMPLETE!\")\n",
        "print(\"=\"*70)\n",
        "print(\"\\nTo download the dendrogram, run in a new cell:\")\n",
        "print(\"  from google.colab import files\")\n",
        "print(\"  files.download('/content/dendrogram_single_linkage.png')\")"
      ],
      "execution_count": 6,
